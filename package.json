{
  "name": "awesome-color-picker",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "webpack --progress --colors --watch",
    "build": "webpack",
    "test": "jest --no-cache"
  },
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "testRegex": "(/test/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.6",
    "@types/jest": "^20.0.8",
    "@types/react": "^16.0.0",
    "@types/react-dom": "^15.5.2",
    "awesome-typescript-loader": "^3.2.2",
    "css-loader": "^0.28.7",
    "enzyme": "^2.9.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.0.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "react-test-renderer": "^15.6.1",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "ts-jest": "^20.0.14",
    "typescript": "^2.4.2",
    "typings-for-css-modules-loader": "^1.5.0"
  }
}